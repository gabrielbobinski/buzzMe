
<html>    
<div style="background-image: url('/images/');background-size:cover;height:100vh;width:100vw;position:fixed;">
</div>

<div class="menue">
    <div class="clips">CLIPS</div>
    <div class="smallLogoDiv" onclick="window.location.replace('article1')"></div>
    <div class="logoDiv"></div><div class="thinLine"></div>
</div>

<div class="rightDiv1" id="addContainerRight">
    <iframe style="width:120px;height:240px;" marginwidth="0" marginheight="0" scrolling="no" frameborder="0" src="//ws-eu.amazon-adsystem.com/widgets/q?ServiceVersion=20070822&OneJS=1&Operation=GetAdHtml&MarketPlace=DE&source=ac&ref=tf_til&ad_type=product_link&tracking_id=buzzmezone06-21&marketplace=amazon&region=DE&placement=B007Z7UENE&asins=B007Z7UENE&linkId=LJ4Y523CZQWLTN6H&show_border=false&link_opens_in_new_window=true&price_color=333333&title_color=209BFF&bg_color=FFFFFF">
    </iframe>
</div>

<div class="leftDiv1" id="addContainerLeft"></div>


<div class="centralDiv" style="top: 12px;">
    

    {{#articlesData}}

    <div  id="article_{{id}}" class="article" style="background-image: url('/images/article_{{id}}/buzzImg_1.jpg');">
        <div id="backward_{{id}}" class="backward" onclick="backPicture({{id}}, {{numberofpages}})"></div>
        <div id="forward_{{id}}" class="forward" onclick="nextPicture({{id}}, {{numberofpages}})"></div>
        <div id="article_blur_{{id}}" class="blurr" style="background-image: url('/images/article_{{id}}/buzzImg_blurr.jpg');"onmouseover="articleOver({{id}})" onmouseout="articleOut({{id}})" onclick="nextPicture({{id}}, {{numberofpages}})"></div>

        <div id="article_dark_{{id}}" class="articleTitle" >
            <div style="width:100%;height:28px; padding-left: 30px; padding-right: 30px;"></div>{{title}}</div>
        
        <div class="articleMenue">

            <a href="http://twitter.com/home?status=http://buzzme.zone/socialArticle/{{id}} via @BuzzMeZone" data-via="BuzzMeZone" onclick="javascript:window.open(this.href,'', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=400,width=555,left=250,top=200');return false;">
            <div class="socialButton" style="margin-left:6px; background-image: url('/images/twitter.jpg');background-size:cover;" ></div></a>
       
            <a href="https://www.facebook.com/sharer/sharer.php?u=http://buzzme.zone/socialArticle/{{id}}" onclick="javascript:window.open(this.href,'', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=400,width=555,left=250,top=200');return false;"><div class="socialButton" style="background-image: url('/images/facebook.jpg'); background-size:cover;"></div></a>
 
            <a href="https://www.pinterest.com/pin/create/link/?url=http://www.buzzme.zone/images/article_1/buzzImg_1.jpg" onclick="javascript:window.open(this.href,'', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=400,width=555,left=250,top=200');return false;">
            <div class="socialButton" style="background-image: url('/images/pinterest.jpg');background-size:cover;"></div>
            </a>

            <a href="https://plus.google.com/share?url=http://buzzme.zone/socialArticle/{{id}}" onclick="javascript:window.open(this.href,'', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=400,width=555,left=250,top=200');return false;">
            <div class="socialButton" style="background-image: url('/images/google+.jpg');background-size:cover;"></div></a>
        </div>
        
    </div>
    
    <div class="googleContainer"></div>
    
    {{/articlesData}}
    
    <div class="buttonImpressum" onclick="window.location.replace('/impressum')">IMPRESSUM</div>
    
</div>
</html>
